        "start": "serve -s ./build",
        "build": "webpack --mode production",
        "test": "jest -w 1 \"-- --coverage\"",
        "test:CI": "jest -w 1 \"-- --coverage\"",
        "coveralls": "cat ./coverage/lcov.info | coveralls",
        "test:watch": "jest --watch",
        "cypress": "cypress run",
        "cy:open": "cypress open",
        "lint": "eslint ./",
        "lint-fix": "eslint ./ --fix"
