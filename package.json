{
  "name": "ah-jawans-frontend",
  "version": "1.0.0",
  "description": "a community of like minded authors to foster inspiration and innovation by leveraging the modern web.",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --mode development --open",
    "start": "serve -s ./build",
    "build": "webpack --mode production",
    "test": "jest --watchAll \"-- --coverage\"",
    "test:CI": "jest \"-- --coverage\"",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andela/ah-jawans-frontend.git"
  },
  "author": "Jawan Developers",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andela/ah-jawans-frontend/issues"
  },
  "homepage": "https://github.com/andela/ah-jawans-frontend#readme",
  "devDependencies": {
    "enzyme": "3.10.0",
    "enzyme-to-json": "3.4.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-react": "^7.14.3",
    "jest": "24.8.0",
    "nyc": "^14.1.1",
    "redux-devtools": "^3.5.0"
  },
  "dependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "axios": "^0.19.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "24.8.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-react-jsx-img-import": "^0.1.4",
    "bootstrap": "^4.3.1",
    "chai": "^4.2.0",
    "css-loader": "^3.2.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "file-loader": "^4.2.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.4",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-thunk": "^2.3.0",
    "sass-loader": "^7.2.0",
    "serve": "^11.1.0",
    "style-loader": "^1.0.0",
    "url-loader": "^2.1.0",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.8.0"
  },
  "jest": {
    "verbose": true,
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__mocks__/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/src/__mocks__/styleMock.js"
    },
    "setupFilesAfterEnv": [
      "./config/enzymeConfig.js"
    ],
    "testPathIgnorePatterns": [
      "./src/__mocks__/"
    ],
    "collectCoverageFrom": [
      "<rootDir>/**/*.js",
      "!<rootDir>/webpack.config.js",
      "!<rootDir>/node_modules",
      "!<rootDir>/src/index.js",
      "!<rootDir>/coverage/**/*.*",
      "!<rootDir>/build/**/*.*",
      "!<rootDir>/dist/**/*.*"
    ]
  }
}
